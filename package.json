{
  "name": "neonhs",
  "main": "index.tsx",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "clean": "rm -rf .expo node_modules",
    "test": "jest --watchAll",
    "typecheck": "tsc --noEmit"
